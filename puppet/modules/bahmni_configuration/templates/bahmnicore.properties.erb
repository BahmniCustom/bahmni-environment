#Make sure this directory exists
bahmnicore.images.directory=<%=scope['config::patientImagesDirectory']%>
bahmnicore.urls.patientimages=<%=scope['config::patientImagesUrl']%>
bahmnicore.documents.baseDirectory=<%=scope['config::documentBaseDirectory']%>

<% if scope['config::bahmni_openerp_required'] == "true" -%>
openerp.port=<%=@openERPPort%>
openerp.host=<%=@openERPHost%>
openerp.database=<%=@openERPDatabase%>
openerp.user=<%=@openERPUser%>
openerp.password=<%=@openERPPassword%>
openerp.connectionTimeoutInMilliseconds=<%=@openERPConnectTimeout%>
openerp.replyTimeoutInMilliseconds=<%=@openERPReadTimeout%>
<% end -%>
